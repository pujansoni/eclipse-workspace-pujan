package ShoppingCartAssignment;

import java.util.*;

public class ShoppingCart {
	
	public void getShoppingCartData(ArrayList itemsAl) {
		HashMap<Double,String> hm=new HashMap<Double,String>();
		for(int i=0; i<itemsAl.size(); i++) {
	 		Object obj = itemsAl.get(i);
			if (obj instanceof Book) {
				hm.put(((Book) obj).getPrice(), ((Book) obj).getData());
			} else if (obj instanceof GiftCard) {
				hm.put(((GiftCard) obj).getPrice(), ((GiftCard) obj).getData());
			}
		}
		TreeMap<Double, String> tm =  new TreeMap<Double, String>(hm);
		System.out.println(tm);
	}

}