public class Book extends Item {
	String aName, title;
	
	Book(String aName, String title, double price){
		super();
		super.setPrice(price);
		this.aName = aName;
		this.title = title;
	}
	
	public String getData() {
		return "Book " + "(" + this.title + " - " + this.aName + ")";
	}
}